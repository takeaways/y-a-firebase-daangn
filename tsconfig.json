{
  "compilerOptions": {
    "target": "es5",
    "module": "ESNext",
    "strict": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "~*": ["./src"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
